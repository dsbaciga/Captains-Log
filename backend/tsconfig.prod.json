{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "strict": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": false,
    "noEmitOnError": true,
    "sourceMap": false,
    "declaration": false,
    "declarationMap": false,
    "skipLibCheck": true,
    "noImplicitAny": false
  },
  "ts-node": {
    "transpileOnly": true
  }
}
